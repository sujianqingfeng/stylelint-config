{
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true,
    "source.fixAll.eslint": true
  },
  "stylelint.validate": ["css", "scss", "vue"],
  "eslint.validate": [
    "javascript",
    "typescript",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.words": [
    "stylelint",
    "stylelintrc",
    "sujian"
  ]
}